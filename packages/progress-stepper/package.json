{
  "name": "@layera/progress-stepper",
  "version": "1.0.0",
  "description": "Enterprise progress stepper component for Layera LEGO system",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "verify": "npm run typecheck && npm run lint --max-warnings=0"
  },
  "keywords": [
    "layera",
    "stepper",
    "progress",
    "react",
    "typescript",
    "lego"
  ],
  "author": "Layera",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@mui/material": "^5.15.0",
    "@mui/icons-material": "^5.15.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@layera/tolgee": "workspace:*",
    "@layera/constants": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}