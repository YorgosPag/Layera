{
  "name": "@layera/map-core",
  "version": "1.0.0",
  "description": "Layera Map Core - Enterprise OpenStreetMap components following ARXES compliance",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@layera/geo-core": "workspace:*",
    "@layera/layout": "workspace:*",
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "tsup": "^8.5.0",
    "@types/leaflet": "^1.9.12",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9"
  },
  "peerDependencies": {
    "leaflet": "^1.9.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/layera/layera.git",
    "directory": "packages/map-core"
  },
  "keywords": [
    "layera",
    "map",
    "openstreetmap",
    "leaflet",
    "enterprise",
    "kados"
  ],
  "author": "Layera Team",
  "license": "MIT"
}