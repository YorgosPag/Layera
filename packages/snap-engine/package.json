{
  "name": "@layera/snap-engine",
  "version": "1.0.0",
  "description": "High-performance spatial snapping engine with R-tree indexing for Layera LEGO architecture",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "layera",
    "lego",
    "snap",
    "spatial",
    "geometry",
    "rtree",
    "snapping",
    "gis",
    "autocad",
    "magnetic",
    "typescript"
  ],
  "author": "Layera Team",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@layera/constants": "workspace:*",
    "@layera/error-boundary": "workspace:*",
    "@layera/file-transformation": "workspace:*",
    "@layera/cad-processing": "workspace:*",
    "rbush": "^4.0.1",
    "rbush-knn": "^3.0.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/rbush": "^3.0.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "eslint": "^8.0.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}