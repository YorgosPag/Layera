{
  "name": "@layera/navigation-handlers",
  "version": "1.0.0",
  "description": "Enterprise navigation handlers - Single source of truth για navigation behavior στο Layera ecosystem",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "navigation",
    "handlers",
    "react",
    "hooks",
    "layera",
    "lego"
  ]
}