{
  "name": "@layera/device-frames",
  "version": "1.0.0",
  "description": "Enterprise device frames and simulator for responsive testing",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts --external react",
    "dev": "tsup src/index.tsx --format cjs,esm --dts --external react --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "device",
    "frames",
    "simulator",
    "responsive",
    "testing"
  ],
  "author": "Layera Team",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.26",
    "tsup": "^7.3.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "react-device-frameset": "^1.3.4"
  }
}