{
  "name": "@layera/file-compression",
  "version": "1.0.0",
  "description": "Advanced file compression and optimization system for Layera LEGO architecture",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "layera",
    "lego",
    "compression",
    "optimization",
    "file-processing",
    "typescript",
    "react"
  ],
  "author": "Layera Team",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@layera/notifications": "workspace:*",
    "@layera/loading": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "eslint": "^8.0.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
